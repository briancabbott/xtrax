<config>
    <sourceValidation failOnMissingSources="true" failOnIncompleteSources="true">
    </sourceValidation>
    <addArtifact>
        <artifact>
            <filename>ArbitraryCopiedCode</filename>
            <declaredLicense>
                <license>
                    <name>CC-BY-SA-3.0</name>
                </license>
            </declaredLicense>
            <isProprietary>false</isProprietary>
            <matchState>EXACT</matchState>
        </artifact>
    </addArtifact>
    <setFinalLicenses>
        <setFinalLicense>
            <artifactSelector>
                <mavenCoordinates>
                    <groupId>com.proprietary.software</groupId>
                    <artifactId>system</artifactId>
                    <version>1.0.0</version>
                </mavenCoordinates>
            </artifactSelector>
            <licenseStatement op="AND">
                <license>
                    <name>PropL</name>
                    <longName>A proprietary License</longName>
                    <text>a very long license text with custom information only relevant for this project.</text>
                </license>
                <licenseStatement op="OR">
                    <license>
                        <name>otherLicense</name>
                    </license>
                    <license>
                        <name>thirdLicense</name>
                    </license>
                </licenseStatement>
            </licenseStatement>
        </setFinalLicense>
    </setFinalLicenses>
</config>
