    <sourceValidation failOnMissingSources="true" failOnIncompleteSources="true">
    </sourceValidation>
    <removeArtifact>
        <artifactSelector>
            <filename>commons-lang-2.0.jar</filename>
        </artifactSelector>
    </removeArtifact>
