        <setFinalLicense>
            <artifactSelector>
                <filename>commons-lang-2.0.jar</filename>
            </artifactSelector>
            <license>
                <name>Apache-2.0</name>
            </license>
        </setFinalLicense>
    </setFinalLicenses>
